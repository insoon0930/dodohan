{
  "hosting": {
    "public": "build/web",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "rewrites": [
      {
        "source": "**",
        "destination": "/index.html"
      }
    ],
    "emulators": {
      "hosting": {
        "port": 5002,
        "host": "0.0.0.0"
      }
    }
  }
}
